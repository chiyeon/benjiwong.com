<div class="project">
   <h2>{{ project.title }}</h2>
   <p class="tagline">{{ project.tagline }}</p>
   <p class="description">{{ project.description }}</p>
   {% if project.images and project.images.length %}
      <img src="{{ "/assets/imgs/projects/" + project.images[0] }}" />
   {% endif %}
</div>
